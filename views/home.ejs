<%- include('partials/header') -%>
<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% postLog.forEach(function(post) { %>
  <h2><%= post.title%></h2>
  <p><%= post.text.slice(0,100) + "... "%><a href="/posts/<%= post._id %>">Read More</a></p>
  <p class="small text-muted">
    Posted on <%= post.updated.toDateString() %>
    </p>
<%});%>

<%- include('partials/footer') -%>
